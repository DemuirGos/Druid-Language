<?xml version="1.0" encoding="utf-8" ?>
<Settings>
  <Current>
    <configuration value="English"/>
  </Current>
  <Localizations>
    <lang Name="English">
      <keywords>
        <keyword key="Boolean"  map="Boolean"    type="Type"/>
        <keyword key="Decimal"  map="Decimal"    type="Type"/>
        <keyword key="Word"     map="Word"       type="Type"/>

        <keyword map="FALSE"    key="False"      type="Value"/>
        <keyword map="TRUE"     key="True"       type="Value"/>
        
        <keyword key="Begin"    map="BEGIN"      type="Block"/>
        <keyword key="Then"     map="THEN"       type="Block"/>
        <keyword key="Else"     map="ELSE"       type="Keyword"/>
        <keyword key="End"      map="END"        type="Block"/>
        <keyword key="If"       map="IF"         type="Keyword"/>
        <keyword key="While"    map="WHILE"      type="Keyword"/>
        <keyword key="To"       map="TO"         type="Accessory"/>
        <keyword key="From"     map="FROM"       type="Accessory"/>
        <keyword key="Do"       map="DO"         type="Block"/>
        <keyword key="For"      map="FOR"        type="Keyword"/>
        <keyword key="By"       map="BY"         type="Accessory"/>
        
        <keyword key="Print"    map="PRINT"      type="Function"/>
        <keyword key="Open"     map="OPEN"       type="Function"/>
        <keyword key="Read"     map="READ"       type="Function"/>
        <keyword key="Include"  map="IMPORT"     type="Function"/>
        
        <keyword key="Function" map="FUNCTION"   type="Keyword"/>
        <keyword map="ARRAY"    key="Array"      type="Type"/>
        <keyword key="Variable" map="VARIABLE"   type="Keyword"/>
        <keyword key="As"       map="OFTYPE"     type="Accessory"/>
        <keyword key="Continue" map="SKIP"       type="Accessory"/>
        <keyword key="Break"    map="EXIT"       type="Accessory"/>
        <keyword key="Return"   map="RETURN"     type="Accessory"/>

        <keyword key="Create"   map="MAKE"       type="Keyword"/>
        <keyword key="Delete"   map="DELETE"     type="Keyword"/>
        <keyword key="Type"     map="TYPE"       type="Keyword"/>
        <keyword key="Member"   map="METHOD"     type="Keyword"/>
      </keywords>
      <operators>
        <symbol map="SET"           key=":="         />
        <symbol map="APPEND"        key="::"         />
        <symbol map="REMOVE"        key=">>"         />
        <symbol map="POWER"         key="^"          />
        <symbol map="EQUAL"         key="="          />
        <symbol map="BIGGER"        key="&gt;"       />
        <symbol map="SMALLER"       key="&lt;"       />
        <symbol map="DIFF"          key="&lt;&gt;"   />
        <symbol map="PLUS"          key="+"          />
        <symbol map="MINUS"         key="-"          />
        <symbol map="DIVIDE"        key="/"          />
        <symbol map="MULTIPLY"      key="*"          />
        <symbol map="REMAINDER"     key="%"          />
        <symbol map="OR"            key="|"          />
        <symbol map="AND"           key="&amp;"      />
        <symbol map="NOT"           key="!"          />
        <symbol map="ACCESS"        key="->"         />
      </operators>
      </lang> 
  </Localizations>
  <symbols isOverloadable="False">
    <symbol map="START_ARG"     key="("          />
    <symbol map="END_ARG"       key=")"          />
    <symbol map="START_ARRAY"   key="["          />
    <symbol map="END_ARRAY"     key="]"          />
    <symbol map="NEXT_ARG"      key=","          />
    <symbol map="QUOTE"         key="'"          />
    <symbol map="SPACE"         key=" "          />
    <symbol map="END_STATEMENT" key="&#10;"      />
    <symbol map="END_STATEMENT" key="&#13;"      />
    <symbol map="END_STATEMENT" key="&#13;&#10;" />
    <symbol map="COMMENT"       key="//"         />
    <symbol map="EMPTY"         key="\0"         />
  </symbols>
</Settings>